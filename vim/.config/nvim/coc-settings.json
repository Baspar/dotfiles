{
  "coc.preferences.formatOnSaveFiletypes": ["scala"],
  "diagnostic": {
    "errorSign": "! ",
    "warningSign": "? ",
    "infoSign": "~ ",
    "hintSign": "- "
  },
  "eslint.filetypes": ["javascript", "javascriptreact", "typescript"],
  "languageserver": {
    "clojure-lsp": {
      "command": "bash",
      "args": ["-c", "clojure-lsp"],
      "filetypes": ["clojure"],
      "rootPatterns": ["project.clj"],
      "additionalSchemes": ["jar", "zipfile"],
      "trace.server": "verbose",
      "initializationOptions": {}
    }
  },
  "list.normalMappings": {
    "<C-c>": "do:exit"
  },
  "list.insertMappings": {
    "<C-c>": "do:exit"
  }
}
